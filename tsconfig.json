{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "es2015", "es2017", "dom.iterable"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"],
      "@/*": ["src/*"]
    }
  },
  "files": [
    "src/svg/nheads.png"
  ],
  "include": [
    ".d.ts",
    "./src/**/*"
  ]
}
